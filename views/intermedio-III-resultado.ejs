<% include _header %>
<div class="container">
    <div class="jumbotron">
        <h1>Terminaste Intermédiare III</h1>
        <p class="lead">Concluiste correctamente el nivel de Intermédiare III</p>
        <div class="form-inline">
            <label><b>Resultado: </b></label>
            <label> <%= currentUsuario.inter3 %></label>
            <label>/9</label>
        </div>
        <% if(currentUsuario.inter3 >= 6) {%> 
            <a class="btn btn-outline-success" href="/avanzado-I">Continuar</a>
        <% } else {%>
            <a class="btn btn-outline-danger" href="/etudiantes/<%= currentUsuario.username %>">Finalizar</a>
        <% } %>
    </div>
</div>
<% include _footer %>